                  MD Git;AEC;Workplace Learning;\_Mark Up;\_Cataloged}
}

@INPROCEEDINGS{Chang2017-qn,
  title      = {Evaluating the effect of tangible virtual reality on spatial
                perspective taking ability},
  author     = {Chang, Jack Shen-Kuen and Yeboah, Georgina and Doucette, Alison
                and Clifton, Paul and Nitsche, Michael and Welsh, Timothy and
                Mazalek, Ali},
  booktitle  = {Proceedings of the 5th Symposium on Spatial User Interaction},
